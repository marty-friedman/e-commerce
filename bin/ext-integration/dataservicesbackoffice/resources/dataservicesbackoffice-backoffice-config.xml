<config xmlns="http://www.hybris.com/cockpit/config" xmlns:na="http://www.hybris.com/cockpitng/config/notifications">

	<!-- dataservicesbackoffice UI configuration should be provided here -->
	<context type="AuditReportConfig" component="editorareaactions" merge-by="type">
		<y:actions xmlns:y="http://www.hybris.com/cockpit/config/hybris">
			<y:group qualifier="common">
				<y:label>actiongroup.common</y:label>
				<y:action action-id="de.hybris.platform.dataservicesbackoffice.actions.exportxsd" property="currentObject.code"/>
			</y:group>
		</y:actions>
	</context>

	<context component="notification-area" source="de.hybris.platform.dataservicesbackoffice.actions.exportxsd" merge-by="source">
		<na:notification-area>
			<na:notifications eventType="dataservices.backoffice.unable.generate.report" level="FAILURE">
				<na:message>dataservices.backoffice.unable.generate.report</na:message>
			</na:notifications>
		</na:notification-area>
	</context>

</config>
