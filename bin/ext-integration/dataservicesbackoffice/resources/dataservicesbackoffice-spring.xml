<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<alias name="defaultAuditReportXSDGenerationService" alias="auditReportXSDGenerationService"/>
	<bean id="defaultAuditReportXSDGenerationService"
		  class="de.hybris.platform.dataservicesbackoffice.audit.impl.DefaultAuditReportXSDGenerationService">
		<property name="auditConfigService" ref="auditConfigService"/>
		<property name="typeService" ref="typeService"/>
	</bean>
	
	<alias name="defaultXSDImpexHeaderGenerationService" alias="xsdImpexHeaderGenerationService"/>
	<bean id="defaultXSDImpexHeaderGenerationService" class="de.hybris.platform.dataservicesbackoffice.audit.impl.DefaultXSDImpexHeaderGenerationService">
	<property name="excludeTypeList">
			<list>
				<value>simpleHybrisField</value>
				<value>localizedHybrisField</value>
			</list>
		</property>
	</bean>
</beans>
